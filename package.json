{
  "name": "homesforsale",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "scripts": {
    "w-node": "nodemon ./dist/index.js",
    "ts": "ts-node-dev --respawn ./app/index.ts",
    "ts-prod": "tsc",
    "w-ts": "tsc -w",
    "js": "webpack --watch --mode development",
    "js-prod": "webpack",
    "css": "postcss ./app/public/css/tailwind.css -o ./app/public/css/app.css --watch",
    "css-prod": "postcss ./app/public/css/tailwind.css -o ./dist/public/css/app.css",
    "development": "concurrently \"npm run ts\" \"npm run css\" \"npm run js\"",
    "start": "full-build && node ./dist/index.js",
    "full-build": "concurrently \"npm run ts-prod\" \"npm run css-prod\" \"npm run js-prod\"",
    "db:import:dev": "ts-node ./app/seeders/seeder.ts -i",
    "db:delete:dev": "ts-node ./app/seeders/seeder.ts -d",
    "db:import": "node ./dist/seeders/seeder.js -i",
    "db:delete": "node ./dist/seeders/seeder.js -d"
  },
  "author": "Brando Arias",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.1",
    "dropzone": "^5.9.3",
    "express": "^4.18.1",
    "express-jwt": "^7.7.5",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.1",
    "html-to-text": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "juice": "^8.0.0",
    "moment": "^2.29.3",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.6",
    "pug": "^3.0.2",
    "sequelize": "^6.21.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/connect-flash": "^0.0.37",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "^6.0.4",
    "@types/express-session": "^1.17.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.4",
    "@types/pug": "^2.0.6",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "nodemon": "^2.0.16",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "tailwindcss": "^3.1.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}

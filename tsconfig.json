{
    "exclude": [
      "node_modules",
      "node_modules",
      "./node_modules",
      "./node_modules/*",
      "./node_modules/@types/node/index.d.ts",
      "./app/interfaces/**/*.ts"
    ],
    "compilerOptions": {
      "baseUrl": ".",
      "typeRoots": [
        "@types",
        "./node_modules/@types"
      ],
      "paths": {
        "*": [
          "node_modules",
          "app/types/*"
        ]
      },
      "target": "es6",
      "module": "commonjs",
      "outDir": "./dist",
      "strict": true,
      "strictPropertyInitialization": false,
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "skipLibCheck": true,
      "types": []
    }
  }